<div class='row-fluid about_text'>
  <div class='span8 offset2 aboutframe'>
    <div class='row-fluid about_text'>
    <% parts = [@t.about['1'], @t.about['2']] %>
    <% for part in parts : %>
        <h3>
          <%= part.title %>
        </h3>
        <p itemprop="description">
        <%- part.description %>
        </p>
    <% end %>
    </div>
  </div>
</div>
