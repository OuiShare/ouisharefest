<div class="row-fluid">
  <div class="registerright span8 offset2">
		<div class='row participate_section'>
			<h2><%= @t.sections.participate %></h2>
				<div id='registerbutton' class='call'>
					<a href="http://www.weezevent.com/ouishare-fest" target="_blank">
						<p>Book your tickets now!</p>
					</a>
				</div>
				<p>
					<%- @t.participate.first %>
				</p>
				<p>
					<%- @t.participate.second %>
				</p>
				<p>
